#!/bin/bash

# occ - OpenShift Console wrapper for oc command
# Acts as a transparent wrapper around oc, but shows relevant console URLs on stderr

# add this line to your .bashrc to enable auto-completion
# complete -F __start_oc occ

# Function to get current namespace from oc context
get_current_namespace() {
    oc config get-contexts --no-headers 2>/dev/null | grep '^\*' | awk '{print $5}' | head -1
}

# Function to get console base URL
get_console_url() {
    oc whoami --show-console 2>/dev/null
}

# Function to generate the best console URL for a command
generate_console_url() {
    local cmd="$1"
    shift
    local args=("$@")
    
    # Handle special cases first
    if [[ "$cmd" == "whoami" ]]; then
        # Handle 'oc whoami' - link to user preferences
        local console_base=$(get_console_url)
        if [[ -n "$console_base" ]]; then
            echo "ğŸŒ ${console_base}/user-preferences" >&2
        fi
        return
    fi
    
    if [[ "$cmd" == "help" ]]; then
        # Handle 'oc help' - link to Red Hat CLI documentation
        echo "ğŸŒ https://docs.redhat.com/en/documentation/openshift_container_platform/4.19/html-single/cli_tools/index" >&2
        return
    fi
    
    if [[ "$cmd" == "api-resources" ]]; then
        # Handle 'oc api-resources' - link to API explorer
        local console_base=$(get_console_url)
        if [[ -n "$console_base" ]]; then
            echo "ğŸŒ ${console_base}/api-explorer" >&2
        fi
        return
    fi
    
    if [[ "$cmd" == "adm" && "${args[0]}" == "top" && "${args[1]}" == "pods" ]]; then
        # Handle 'oc adm top pods' - link to monitoring dashboard
        local console_base=$(get_console_url)
        if [[ -n "$console_base" ]]; then
            local namespace=$(get_current_namespace)
            if [[ -n "$namespace" ]]; then
                echo "ğŸŒ ${console_base}/dev-monitoring/ns/$namespace?dashboard=dashboard-k8s-resources-namespace" >&2
            fi
        fi
        return
    fi
    
    # Handle 'exec' command - link to pod terminal
    if [[ "$cmd" == "exec" ]]; then
        local console_base=$(get_console_url)
        if [[ -n "$console_base" ]]; then
            local pod_name=""
            local namespace=""
            
            # Parse exec command arguments to find pod name and namespace
            local i=0
            while [[ $i -lt ${#args[@]} ]]; do
                case "${args[$i]}" in
                    -n|--namespace)
                        i=$((i + 1))
                        if [[ $i -lt ${#args[@]} ]]; then
                            namespace="${args[$i]}"
                        fi
                        ;;
                    -c|--container|-i|--stdin|-t|--tty)
                        # Skip these flags and their values (except -i and -t which are boolean)
                        if [[ "${args[$i]}" == "-c" || "${args[$i]}" == "--container" ]]; then
                            i=$((i + 1))
                        fi
                        ;;
                    --)
                        # Stop parsing after -- (command separator)
                        break
                        ;;
                    -*)
                        # Skip other flags
                        ;;
                    *)
                        # First non-flag argument should be the pod name
                        if [[ -z "$pod_name" ]]; then
                            pod_name="${args[$i]}"
                        fi
                        ;;
                esac
                i=$((i + 1))
            done
            
            # If no namespace specified, get current namespace from context
            if [[ -z "$namespace" ]]; then
                namespace=$(get_current_namespace)
            fi
            
            # Generate terminal URL if we have pod name and namespace
            if [[ -n "$pod_name" && -n "$namespace" ]]; then
                echo "ğŸŒ ${console_base}/k8s/ns/$namespace/pods/$pod_name/terminal" >&2
            fi
        fi
        return
    fi
    
    # Handle 'run' command - link to pod creation form
    if [[ "$cmd" == "run" ]]; then
        local console_base=$(get_console_url)
        if [[ -n "$console_base" ]]; then
            local namespace=""
            
            # Parse run command arguments to find namespace
            local i=0
            while [[ $i -lt ${#args[@]} ]]; do
                case "${args[$i]}" in
                    -n|--namespace)
                        i=$((i + 1))
                        if [[ $i -lt ${#args[@]} ]]; then
                            namespace="${args[$i]}"
                        fi
                        ;;
                esac
                i=$((i + 1))
            done
            
            # If no namespace specified, get current namespace from context
            if [[ -z "$namespace" ]]; then
                namespace=$(get_current_namespace)
            fi
            
            if [[ -n "$namespace" ]]; then
                echo "ğŸŒ ${console_base}/k8s/ns/$namespace/pods/~new" >&2
            fi
        fi
        return
    fi
    
    # Handle 'create' command - link to appropriate creation forms
    if [[ "$cmd" == "create" ]]; then
        local console_base=$(get_console_url)
        if [[ -n "$console_base" ]]; then
            local resource_type=""
            local secret_type=""
            local namespace=""
            
            # Parse create command arguments
            local i=0
            while [[ $i -lt ${#args[@]} ]]; do
                case "${args[$i]}" in
                    -n|--namespace)
                        i=$((i + 1))
                        if [[ $i -lt ${#args[@]} ]]; then
                            namespace="${args[$i]}"
                        fi
                        ;;
                    # Resources available through 'oc create' command
                    build)
                        resource_type="build"
                        ;;
                    clusterresourcequota)
                        resource_type="clusterresourcequota"
                        ;;
                    clusterrole|clusterroles)
                        resource_type="clusterrole"
                        ;;
                    clusterrolebinding|clusterrolebindings)
                        resource_type="clusterrolebinding"
                        ;;
                    configmap|configmaps)
                        resource_type="configmap"
                        ;;
                    cronjob|cronjobs)
                        resource_type="cronjob"
                        ;;
                    deployment|deployments)
                        resource_type="deployment"
                        ;;
                    deploymentconfig|deploymentconfigs)
                        resource_type="deploymentconfig"
                        ;;
                    identity|identities)
                        resource_type="identity"
                        ;;
                    imagestream|imagestreams)
                        resource_type="imagestream"
                        ;;
                    imagestreamtag|imagestreamtags)
                        resource_type="imagestreamtag"
                        ;;
                    ingress|ingresses)
                        resource_type="ingress"
                        ;;
                    job|jobs)
                        resource_type="job"
                        ;;
                    namespace|namespaces)
                        resource_type="namespace"
                        ;;
                    poddisruptionbudget|poddisruptionbudgets|pdb)
                        resource_type="poddisruptionbudget"
                        ;;
                    priorityclass|priorityclasses)
                        resource_type="priorityclass"
                        ;;
                    quota|resourcequota|resourcequotas)
                        resource_type="resourcequota"
                        ;;
                    role|roles)
                        resource_type="role"
                        ;;
                    rolebinding|rolebindings)
                        resource_type="rolebinding"
                        ;;
                    route|routes)
                        resource_type="route"
                        ;;
                    secret|secrets)
                        resource_type="secret"
                        # Check for secret type
                        if [[ $((i + 1)) -lt ${#args[@]} ]]; then
                            case "${args[$((i + 1))]}" in
                                generic)
                                    secret_type="generic"
                                    ;;
                                docker-registry)
                                    secret_type="docker-registry"
                                    ;;
                                tls)
                                    secret_type="tls"
                                    ;;
                            esac
                        fi
                        ;;
                    service|services)
                        resource_type="service"
                        ;;
                    serviceaccount|serviceaccounts)
                        resource_type="serviceaccount"
                        ;;
                    token)
                        resource_type="token"
                        ;;
                    user|users)
                        resource_type="user"
                        ;;
                    useridentitymapping|useridentitymappings)
                        resource_type="useridentitymapping"
                        ;;
                esac
                i=$((i + 1))
            done
            
            # If no namespace specified, get current namespace from context
            if [[ -z "$namespace" ]]; then
                namespace=$(get_current_namespace)
            fi
            
            # Generate appropriate console URL based on resource type
            if [[ -n "$resource_type" ]]; then
                case "$resource_type" in
                    # Cluster-scoped resources
                    clusterresourcequota)
                        echo "ğŸŒ ${console_base}/k8s/cluster/quota.openshift.io~v1~ClusterResourceQuota/~new" >&2
                        ;;
                    clusterrole)
                        echo "ğŸŒ ${console_base}/k8s/cluster/rbac.authorization.k8s.io~v1~ClusterRole/~new" >&2
                        ;;
                    clusterrolebinding)
                        echo "ğŸŒ ${console_base}/k8s/cluster/rbac.authorization.k8s.io~v1~ClusterRoleBinding/~new" >&2
                        ;;
                    namespace)
                        echo "ğŸŒ ${console_base}/k8s/cluster/core~v1~Namespace/~new" >&2
                        ;;
                    priorityclass)
                        echo "ğŸŒ ${console_base}/k8s/cluster/scheduling.k8s.io~v1~PriorityClass/~new" >&2
                        ;;
                    user)
                        echo "ğŸŒ ${console_base}/k8s/cluster/user.openshift.io~v1~User/~new" >&2
                        ;;
                    identity)
                        echo "ğŸŒ ${console_base}/k8s/cluster/user.openshift.io~v1~Identity/~new" >&2
                        ;;
                    useridentitymapping)
                        echo "ğŸŒ ${console_base}/k8s/cluster/user.openshift.io~v1~UserIdentityMapping/~new" >&2
                        ;;
                    # Namespaced resources
                    *)
                        if [[ -n "$namespace" ]]; then
                            case "$resource_type" in
                                build)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/build.openshift.io~v1~Build/~new" >&2
                                    ;;
                                configmap)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/configmaps/~new" >&2
                                    ;;
                                cronjob)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/cronjobs/~new" >&2
                                    ;;
                                deployment)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/deployments/~new/form" >&2
                                    ;;
                                deploymentconfig)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/apps.openshift.io~v1~DeploymentConfig/~new" >&2
                                    ;;
                                imagestream)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/image.openshift.io~v1~ImageStream/~new" >&2
                                    ;;
                                imagestreamtag)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/image.openshift.io~v1~ImageStreamTag/~new" >&2
                                    ;;
                                ingress)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/ingresses/~new" >&2
                                    ;;
                                job)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/jobs/~new" >&2
                                    ;;
                                poddisruptionbudget)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/poddisruptionbudgets/~new" >&2
                                    ;;
                                resourcequota)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/resourcequotas/~new" >&2
                                    ;;
                                role)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/roles/~new" >&2
                                    ;;
                                rolebinding)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/rolebindings/~new" >&2
                                    ;;
                                route)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/route.openshift.io~v1~Route/~new" >&2
                                    ;;
                                secret)
                                    case "$secret_type" in
                                        generic)
                                            echo "ğŸŒ ${console_base}/k8s/ns/$namespace/secrets/~new/generic" >&2
                                            ;;
                                        docker-registry)
                                            echo "ğŸŒ ${console_base}/k8s/ns/$namespace/secrets/~new/image" >&2
                                            ;;
                                        tls)
                                            echo "ğŸŒ ${console_base}/k8s/ns/$namespace/secrets/~new/tls" >&2
                                            ;;
                                        *)
                                            echo "ğŸŒ ${console_base}/k8s/ns/$namespace/secrets/~new" >&2
                                            ;;
                                    esac
                                    ;;
                                service)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/services/~new" >&2
                                    ;;
                                serviceaccount)
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/serviceaccounts/~new" >&2
                                    ;;
                                token)
                                    # Token creation doesn't have a specific form, link to service accounts
                                    echo "ğŸŒ ${console_base}/k8s/ns/$namespace/serviceaccounts" >&2
                                    ;;
                            esac
                        fi
                        ;;
                esac
            fi
        fi
        return
    fi
    
    # Handle 'explain' command - link to Red Hat API documentation
    if [[ "$cmd" == "explain" ]]; then
        if [[ -n "${args[0]}" ]]; then
            local resource_field="${args[0]}"
            # Extract base resource type (e.g., deployment from deployment.spec.replicas)
            local base_resource="${resource_field%%.*}"
            local docs_base="https://docs.redhat.com/en/documentation/openshift_container_platform/4.19/html-single"
            
            # Map resources to their Red Hat documentation URLs
            case "$base_resource" in
                # Workload APIs
                deployment|deployments)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#deployment-apps-v1" >&2
                    ;;
                pod|pods)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#pod-v1-1" >&2
                    ;;
                replicaset|replicasets)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#replicaset-apps-v1" >&2
                    ;;
                statefulset|statefulsets)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#statefulset-apps-v1" >&2
                    ;;
                daemonset|daemonsets)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#daemonset-apps-v1" >&2
                    ;;
                job|jobs)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#job-batch-v1" >&2
                    ;;
                cronjob|cronjobs)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#cronjob-batch-v1" >&2
                    ;;
                build|builds)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#build-build-openshift-io-v1" >&2
                    ;;
                buildconfig|buildconfigs)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#buildconfig-build-openshift-io-v1" >&2
                    ;;
                buildlog|buildlogs)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#buildlog-build-openshift-io-v1" >&2
                    ;;
                buildrequest|buildrequests)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#buildrequest-build-openshift-io-v1" >&2
                    ;;
                deploymentconfig|deploymentconfigs)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#deploymentconfig-apps-openshift-io-v1" >&2
                    ;;
                deploymentconfigrollback|deploymentconfigrollbacks)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#deploymentconfigrollback-apps-openshift-io-v1" >&2
                    ;;
                deploymentlog|deploymentlogs)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#deploymentlog-apps-openshift-io-v1" >&2
                    ;;
                deploymentrequest|deploymentrequests)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#deploymentrequest-apps-openshift-io-v1" >&2
                    ;;
                replicationcontroller|replicationcontrollers)
                    echo "ğŸŒ ${docs_base}/workloads_apis/index#replicationcontroller-v1" >&2
                    ;;
                controllerrevision|controllerrevisions)
                    echo "ğŸŒ ${docs_base}/metadata_apis/index#controllerrevision-apps-v1" >&2
                    ;;
                podtemplate|podtemplates)
                    echo "ğŸŒ ${docs_base}/template_apis/index#podtemplate-v1" >&2
                    ;;
                
                # Network APIs
                service|services)
                    echo "ğŸŒ ${docs_base}/network_apis/index#service-v1" >&2
                    ;;
                ingress|ingresses)
                    echo "ğŸŒ ${docs_base}/network_apis/index#ingress-networking-k8s-io-v1" >&2
                    ;;
                ingressclass|ingressclasses)
                    echo "ğŸŒ ${docs_base}/network_apis/index#ingressclass-networking-k8s-io-v1" >&2
                    ;;
                route|routes)
                    echo "ğŸŒ ${docs_base}/network_apis/index#route-route-openshift-io-v1" >&2
                    ;;
                endpoints|endpoint)
                    echo "ğŸŒ ${docs_base}/network_apis/index#endpoints-v1" >&2
                    ;;
                endpointslice|endpointslices)
                    echo "ğŸŒ ${docs_base}/network_apis/index#endpointslice-discovery-k8s-io-v1" >&2
                    ;;
                networkpolicy|networkpolicies)
                    echo "ğŸŒ ${docs_base}/network_apis/index#networkpolicy-networking-k8s-io-v1" >&2
                    ;;
                networkattachmentdefinition|networkattachmentdefinitions)
                    echo "ğŸŒ ${docs_base}/network_apis/index#networkattachmentdefinition-k8s-cni-cncf-io-v1" >&2
                    ;;
                multinetworkpolicy|multinetworkpolicies)
                    echo "ğŸŒ ${docs_base}/network_apis/index#multinetworkpolicy-k8s-cni-cncf-io-v1beta1" >&2
                    ;;
                adminnetworkpolicy|adminnetworkpolicies)
                    echo "ğŸŒ ${docs_base}/network_apis/index#adminnetworkpolicy-policy-networking-k8s-io-v1alpha1" >&2
                    ;;
                baselineadminnetworkpolicy|baselineadminnetworkpolicies)
                    echo "ğŸŒ ${docs_base}/network_apis/index#baselineadminnetworkpolicy-policy-networking-k8s-io-v1alpha1" >&2
                    ;;
                egressfirewall|egressfirewalls)
                    echo "ğŸŒ ${docs_base}/network_apis/index#egressfirewall-k8s-ovn-org-v1" >&2
                    ;;
                egressip|egressips)
                    echo "ğŸŒ ${docs_base}/network_apis/index#egressip-k8s-ovn-org-v1" >&2
                    ;;
                egressqos)
                    echo "ğŸŒ ${docs_base}/network_apis/index#egressqos-k8s-ovn-org-v1" >&2
                    ;;
                egressrouter|egressrouters)
                    echo "ğŸŒ ${docs_base}/network_apis/index#egressrouter-network-operator-openshift-io-v1" >&2
                    ;;
                egressservice|egressservices)
                    echo "ğŸŒ ${docs_base}/network_apis/index#egressservice-k8s-ovn-org-v1" >&2
                    ;;
                gateway|gateways)
                    echo "ğŸŒ ${docs_base}/network_apis/index#gateway-gateway-networking-k8s-io-v1" >&2
                    ;;
                gatewayclass|gatewayclasses)
                    echo "ğŸŒ ${docs_base}/network_apis/index#gatewayclass-gateway-networking-k8s-io-v1" >&2
                    ;;
                httproute|httproutes)
                    echo "ğŸŒ ${docs_base}/network_apis/index#httproute-gateway-networking-k8s-io-v1" >&2
                    ;;
                grpcroute|grpcroutes)
                    echo "ğŸŒ ${docs_base}/network_apis/index#grpcroute-gateway-networking-k8s-io-v1" >&2
                    ;;
                referencegrant|referencegrants)
                    echo "ğŸŒ ${docs_base}/network_apis/index#referencegrant-gateway-networking-k8s-io-v1beta1" >&2
                    ;;
                cloudprivateipconfig|cloudprivateipconfigs)
                    echo "ğŸŒ ${docs_base}/network_apis/index#cloudprivateipconfig-cloud-network-openshift-io-v1" >&2
                    ;;
                clusteruserdefinednetwork|clusteruserdefinednetworks)
                    echo "ğŸŒ ${docs_base}/network_apis/index#clusteruserdefinednetwork-k8s-ovn-org-v1" >&2
                    ;;
                userdefinednetwork|userdefinednetworks)
                    echo "ğŸŒ ${docs_base}/network_apis/index#userdefinednetwork-k8s-ovn-org-v1" >&2
                    ;;
                ipamclaim|ipamclaims)
                    echo "ğŸŒ ${docs_base}/network_apis/index#ipamclaim-k8s-cni-cncf-io-v1alpha1" >&2
                    ;;
                ippool|ippools)
                    echo "ğŸŒ ${docs_base}/network_apis/index#ippool-whereabouts-cni-cncf-io-v1alpha1" >&2
                    ;;
                nodeslicepool|nodeslicepools)
                    echo "ğŸŒ ${docs_base}/network_apis/index#nodeslicepool-whereabouts-cni-cncf-io-v1alpha1" >&2
                    ;;
                overlappingrangeipreservation|overlappingrangeipreservations)
                    echo "ğŸŒ ${docs_base}/network_apis/index#overlappingrangeipreservation-whereabouts-cni-cncf-io-v1alpha1" >&2
                    ;;
                podnetworkconnectivitycheck|podnetworkconnectivitychecks)
                    echo "ğŸŒ ${docs_base}/network_apis/index#podnetworkconnectivitycheck-controlplane-operator-openshift-io-v1alpha1" >&2
                    ;;
                adminpolicybasedexternalroute|adminpolicybasedexternalroutes)
                    echo "ğŸŒ ${docs_base}/network_apis/index#adminpolicybasedexternalroute-k8s-ovn-org-v1" >&2
                    ;;
                
                # Storage APIs
                persistentvolume|persistentvolumes|pv)
                    echo "ğŸŒ ${docs_base}/storage_apis/index#persistentvolume-v1" >&2
                    ;;
                persistentvolumeclaim|persistentvolumeclaims|pvc)
                    echo "ğŸŒ ${docs_base}/storage_apis/index#persistentvolumeclaim-v1" >&2
                    ;;
                storageclass|storageclasses)
                    echo "ğŸŒ ${docs_base}/storage_apis/index#storageclass-storage-k8s-io-v1" >&2
                    ;;
                volumeattachment|volumeattachments)
                    echo "ğŸŒ ${docs_base}/storage_apis/index#volumeattachment-storage-k8s-io-v1" >&2
                    ;;
                volumesnapshot|volumesnapshots)
                    echo "ğŸŒ ${docs_base}/storage_apis/index#volumesnapshot-snapshot-storage-k8s-io-v1" >&2
                    ;;
                volumesnapshotclass|volumesnapshotclasses)
                    echo "ğŸŒ ${docs_base}/storage_apis/index#volumesnapshotclass-snapshot-storage-k8s-io-v1" >&2
                    ;;
                volumesnapshotcontent|volumesnapshotcontents)
                    echo "ğŸŒ ${docs_base}/storage_apis/index#volumesnapshotcontent-snapshot-storage-k8s-io-v1" >&2
                    ;;
                csidriver|csidrivers)
                    echo "ğŸŒ ${docs_base}/storage_apis/index#csidriver-storage-k8s-io-v1" >&2
                    ;;
                csinode|csinodes)
                    echo "ğŸŒ ${docs_base}/storage_apis/index#csinode-storage-k8s-io-v1" >&2
                    ;;
                csistoragecapacity|csistoragecapacities)
                    echo "ğŸŒ ${docs_base}/storage_apis/index#csistoragecapacity-storage-k8s-io-v1" >&2
                    ;;
                storagestate|storagestates)
                    echo "ğŸŒ ${docs_base}/storage_apis/index#storagestate-migration-k8s-io-v1alpha1" >&2
                    ;;
                storageversionmigration|storageversionmigrations)
                    echo "ğŸŒ ${docs_base}/storage_apis/index#storageversionmigration-migration-k8s-io-v1alpha1" >&2
                    ;;
                
                # Config and Metadata APIs
                configmap|configmaps)
                    echo "ğŸŒ ${docs_base}/metadata_apis/index#configmap-v1" >&2
                    ;;
                namespace|namespaces)
                    echo "ğŸŒ ${docs_base}/metadata_apis/index#namespace-v1" >&2
                    ;;
                event|events)
                    echo "ğŸŒ ${docs_base}/metadata_apis/index#event-v1" >&2
                    ;;
                lease|leases)
                    echo "ğŸŒ ${docs_base}/metadata_apis/index#lease-coordination-k8s-io-v1" >&2
                    ;;
                binding|bindings)
                    echo "ğŸŒ ${docs_base}/metadata_apis/index#binding-v1" >&2
                    ;;
                componentstatus|componentstatuses)
                    echo "ğŸŒ ${docs_base}/metadata_apis/index#componentstatus-v1" >&2
                    ;;
                apirequestcount|apirequestcounts)
                    echo "ğŸŒ ${docs_base}/metadata_apis/index#apirequestcount-apiserver-openshift-io-v1" >&2
                    ;;
                
                # Security APIs
                secret|secrets)
                    echo "ğŸŒ ${docs_base}/security_apis/index#secret-v1" >&2
                    ;;
                serviceaccount|serviceaccounts)
                    echo "ğŸŒ ${docs_base}/security_apis/index#serviceaccount-v1" >&2
                    ;;
                securitycontextconstraints|scc)
                    echo "ğŸŒ ${docs_base}/security_apis/index#securitycontextconstraints-security-openshift-io-v1" >&2
                    ;;
                certificatesigningrequest|certificatesigningrequests)
                    echo "ğŸŒ ${docs_base}/security_apis/index#certificatesigningrequest-certificates-k8s-io-v1" >&2
                    ;;
                credentialsrequest|credentialsrequests)
                    echo "ğŸŒ ${docs_base}/security_apis/index#credentialsrequest-cloudcredential-openshift-io-v1" >&2
                    ;;
                podsecuritypolicyreview|podsecuritypolicyreviews)
                    echo "ğŸŒ ${docs_base}/security_apis/index#podsecuritypolicyreview-security-openshift-io-v1" >&2
